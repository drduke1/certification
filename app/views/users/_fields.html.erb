<%= render 'shared/error_messages' %>

<%= f.label :name %>
<%= f.text_field :name, class: "input-lg" %>

<%= f.label :email %>
<%= f.text_field :email, class: "input-lg" %>

<%= f.label :password %>
<%= f.password_field :password, class: "input-lg" %>

<%= f.label :password_confirmation, "Confirm Password" %>
<%= f.password_field :password_confirmation, class: "input-lg" %>

<%= f.label :permission, "Permission Level" %>
<%= f.select :permission, [ ["Read Only", "read"], ["IP Voice Telephony", "ip_voice"], ["IP Video Telephony", "ip_video_telephony"], ["Enterprise Gateways", "enterprise_gateways"], ["Consumer ATAs", "consumer_atas"], ["IP PBX", "ip_pbx"], ["Master of All", "all"] ], {}, {:multiple => true, class: "input-lg"} %>
	
<%= f.label :admin %><br>
	<%= f.check_box :admin %>